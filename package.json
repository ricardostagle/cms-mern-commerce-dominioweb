    "config": "^3.3.3",
    "cookie-parser": "^1.4.5",
    "eslint": "^7.11.0",
    "express": "^4.19.2",
    "firebase": "^10.11.0",
    "jest": "29.7.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.2",
    "react-scripts": "^5.0.1",
    "redux-devtools-extension": "^2.13.9",
    "resolve-url-loader": "5.0.0",
    "simple-update-notifier": "2.0.0",
    "stripe": "^15.0.0",
    "styled-components": "^6.1.8",
    "svgo": "^2.8.0",
    "nth-check": "^2.1.1",
    "postcss": "^8.4.38",
    "validator": "^13.5.2",
    "webpack": "^5.91.0",
